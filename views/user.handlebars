	<div class="container">
		<div class="row">
			<div class="col-md-4">
				<img src="{{dataValues.image}}" width="90%">
			</div>

			<div class="col-md-8">
				<h1>{{dataValues.name}}</h1> <!-- user name -->
				<h3>{{data.dataValues.location}}</h3> <!-- user location -->
				<br>
				<br>
				<h4>About</h4>

				<p>{{data.dataValues.description}}</p>

				<h4>Services</h4>
				<div class="servicesDiv">
					{{#each Services}}
						<div class="col-sm-3">
					        <div class="card">
					          	<img class="card-img-top" src="{{this.image}}" alt="Card image cap" width=90%>
					          	<div class="card-block">
					          		<p class="card-text"><strong>{{this.title}}</strong></p>
					            	<p class="card-text">{{this.description}}</p>
					            	
					          	</div>
					        </div>
					    </div> 
					{{/each}}   
				</div>
				
				
				<a class="btn btn-default" href="/service">Add a Service</a>
			</div>
		</div>
